modules = ["nodejs-20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "React Dev Server"

[[workflows.workflow]]
name = "React Dev Server"
author = "agent"
mode = "sequential"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd client && npm run dev -- --host 0.0.0.0 --port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Install Dependencies"
author = 47908771
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pnpm install"

[[workflows.workflow]]
name = "Build Project"
author = 47908771
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pnpm run build"

[[workflows.workflow]]
name = "Preview Build"
author = 47908771
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pnpm run preview"

